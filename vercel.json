{
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/download/(.*)",
      "destination": "https://you-audio-backend.vercel.app/download/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/app/main.py"
    }
  ]
}
